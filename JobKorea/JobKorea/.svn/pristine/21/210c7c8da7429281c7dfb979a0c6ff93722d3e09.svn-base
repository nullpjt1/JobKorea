<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.kosmo.jobkorea.join.dao.JoinDAO">

<!-- 회원가입 -->
<insert id="signup">
	insert into tb_userinfo(
		userId,password,name,email,email2,email_domain,gender,grade,join_in,birth
		,area,phone1,phone2,phone3,worktitle,workarea1,workarea2,workarea3,salary
		,consult_yn,career_year,career_mm,skill_check,add_tech,career_text,significant
		,fileup
	)
	values(
		#{userId}
	,	#{password}
	, 	#{name}
	, 	#{email}
	, 	#{email2}
	, 	#{email_domain}
	, 	#{gender}
	, 	#{grade}
	, 	#{join_in}
	, 	#{birth}
	,	#{area}
	, 	#{phone1}
	, 	#{phone2}
	, 	#{phone3}
	, 	#{worktitle}
	, 	#{workarea1}
	, 	#{workarea2}
	, 	#{workarea3}
	, 	#{salary}
	, 	#{consult_yn}
	, 	#{career_year}
	, 	#{career_mm}
	, 	#{skill_check}
	, 	#{add_tech}
	, 	#{career_text}
	, 	#{significant}
	, 	#{fileup}
	)
</insert>
</mapper>