package kr.kosmo.jobkorea.qna.model;

public class QnaListVO2 {
	
	
	private int wno;
	private int upper_wno;
	private String regId;
	private String title;
	private String note;
	private String reg_date;
	private String update_date;
	private int num;
	
	
	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}

	// 코멘트 
	private int commentsCnt;
	
	public int getWno() {
		return wno;
	}

	public void setWno(int wno) {
		this.wno = wno;
	}

	public int getUpper_wno() {
		return upper_wno;
	}

	public void setUpper_wno(int upper_wno) {
		this.upper_wno = upper_wno;
	}

	public String getRegId() {
